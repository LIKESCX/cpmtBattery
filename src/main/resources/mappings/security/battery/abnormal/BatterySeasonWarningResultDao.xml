  <select id="queryBatterySeasonWarningResult" resultMap="BaseResultMap">
  	SELECT
			operator_id,
			station_id,
			equipment_id,
			connector_id,
			bms_code,
			area_code,
			warning_code,
			SUM(warning_code_times) AS warning_code_times,
			warning_level
		FROM
			`sec_battery_month_warning_result`
		WHERE
			operator_id = #{operatorId,jdbcType=VARCHAR}
		AND 
			statistical_time  between  #{startDate} and #{endDate}
		GROUP BY 
			operator_id,
			station_id,
			equipment_id,
			connector_id,
			area_code,
			bms_code,
			warning_code,
			warning_level
  </select>
  
