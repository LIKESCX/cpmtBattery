    <select id="getDangerCheckSolveByDangerCheckSolve" parameterType="com.cpit.cpmt.dto.security.DangerCheckSolve"
            resultMap="BaseResultMap">

        select c.operator_id, a.station_id, a.station_name, a.area_code, b.equipment_name, b.equipment_id
        from exc_operator_info c
                 left join exc_station_info a on a.operator_id = c.operator_id
                 left join exc_equipment_info b on a.operator_id = b.operator_id
        where c.operator_name = #{operatorName,jdbcType=VARCHAR}
          and b.equipment_id = #{equipmentId,jdbcType=VARCHAR}
          and a.station_name = #{stationName,jdbcType=VARCHAR}
    </select>

