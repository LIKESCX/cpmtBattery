<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.cpit.cpmt.biz.dao.system.UserStreetDao">
  <resultMap id="BaseResultMap" type="com.cpit.cpmt.dto.system.UserStreetKey">
    <id column="user_id" jdbcType="VARCHAR" property="userId" />
    <id column="street_id" jdbcType="INTEGER" property="streetId" />
  </resultMap>

  
  <insert id="insert" parameterType="com.cpit.cpmt.dto.system.UserStreetKey">
    insert into sys_user_street (user_id, street_id)
    values (#{userId,jdbcType=VARCHAR}, #{streetId,jdbcType=INTEGER})
  </insert>

  <delete id="deleteByUserId" parameterType="java.lang.String">
    delete from sys_user_street
    where user_id = #{userId,jdbcType=VARCHAR}
  </delete>


</mapper>